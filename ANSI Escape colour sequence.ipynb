    "# ANSI escape codes for colors and styles\n",
    "RESET = \"\\033[0m\"\n",
    "RED = \"\\033[31m\"\n",
    "GREEN = \"\\033[32m\"\n",
    "BOLD = \"\\033[1m\"\n",
    "HIGHLIGHT = \"\\033[7m\"\n",
    "BLACK = \"\\033[30m\"\n",
    "\n",
    "print(f\"{RED}This is a red message.{RESET}\")\n",
    "print(f\"{BOLD}{GREEN}{BLACK}This is bold and green.{RESET}\")"
   ]
